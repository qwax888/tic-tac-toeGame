<Window x:Class="TIC_TAC_TOE_GAME_.GAME_OVER"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TIC_TAC_TOE_GAME_"
        mc:Ignorable="d"
        Background="#c4c4c4"
        Title="GAME_OVER" Height="294" Width="383">
    <Grid Margin="-58,0,0,-37">
        <Label x:Name="game_over_lb" Content="GAME OVER!" Foreground="#545454" Margin="130,72,57,159" FontWeight="Bold" FontSize="20px" HorizontalContentAlignment="Center"></Label>
        <Label x:Name="have_winner_lb" Content="WE HAVE A WINNER:" Foreground="#545454" Margin="97,114,0,112" FontWeight="Bold" FontSize="20px" RenderTransformOrigin="0.556,0.334" HorizontalContentAlignment="Center"></Label>
        <Label x:Name="winner_lb"  Content="PLAYER 1" Foreground="#545454" Margin="133,161,57,96" FontWeight="Bold" FontSize="20px" RenderTransformOrigin="0.556,0.334" HorizontalContentAlignment="Center"></Label>
    </Grid>


</Window>
